{
  "name": "@tyleraholden/lionne-shared",
  "version": "1.1.7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "UNLICENSED",
  "repository": "git://git@github.com:tyleraholden/lionne-shared.git",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -f tsconfig.tsbuildinfo && rimraf -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch --preserveWatchOutput",
    "serve": "yarn compile && yarn watch",
    "prepublishOnly": "yarn run build",
    "test": "yarn run build",
    "deploy": "npm version patch && npm publish"
  },
  "dependencies": {
    "libphonenumber-js": "^1.9.8",
    "slate": "^0.59.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "firebase": "^7.15.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
